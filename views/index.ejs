<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title>Excel File Viewer</title>
    <style>
        h1 {
            background-color: beige;
            text-align: center;
        }

        table {
            margin-left: auto;
            margin-right: auto;
        }
    </style>
</head>

<body>
    <h1>분석</h1>
    <table>
        <thead>
            <tr>
                <th>id</th>
                <th>referee</th>
                <th>periods.first</th>
                <th>periods.second</th>
                <th>venue.id</th>
                <th>venue.city</th>
            </tr>
        </thead>
        <tbody>
            <% abcd.forEach(function(row) { %>
                <tr>
                    <td>
                        <%= row.id %>
                    </td>
                    <td>
                        <%= row.referee %>
                    </td>
                    <td>
                        <%= row['periods.first'] %>
                    </td>
                    <td>
                        <%= row['periods.second'] %>
                    </td>
                    <td>
                        <%= row['venue.id'] %>
                    </td>
                    <td>
                        <%= row['venue.city'] %>
                    </td>
                </tr>
                <% }); %>
        </tbody>
    </table>
    <form action="search_process">
        <input type="text" name="name" placeholder="miguel ortiz">
        <button type="submit"></button>
    </form>
</body>

</html>